import {TransitionGroup, Transition} from 'intact';
import {makeMessagesStyles} from './styles';

<div class={{"k-messages": true, [makeMessagesStyles()]: true}}>
    <TransitionGroup>
        <Transition v-for={this.messages} v-for-value="Message" key={Message.key}>
            {Message}
        </Transition>
    </TransitionGroup>
</div>
